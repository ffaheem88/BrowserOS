# Test Environment Configuration
NODE_ENV=test

# Server Configuration
PORT=5001

# Test Database Configuration
DATABASE_URL=postgresql://browseros_user:browseros_dev_password@localhost:5432/browseros_test

# Redis Configuration (use separate Redis DB for tests)
REDIS_URL=redis://:browseros_redis_password@localhost:6379/1

# JWT Configuration (shorter expiry for faster tests)
JWT_SECRET=test_jwt_secret_for_testing_only
JWT_REFRESH_SECRET=test_jwt_refresh_secret_for_testing_only
JWT_EXPIRES_IN=5m
JWT_REFRESH_EXPIRES_IN=1d

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# File Storage (MinIO for tests)
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=browseros_minio
S3_SECRET_KEY=browseros_minio_password
S3_BUCKET=browseros-files-test
S3_REGION=us-east-1
