# BrowserOS Backend Environment Configuration Template
# Copy this file to .env and update with your actual values

# Environment
NODE_ENV=development

# Server Configuration
PORT=5000

# Database Configuration
DATABASE_URL=postgresql://browseros_user:browseros_dev_password@localhost:5432/browseros

# Redis Configuration
REDIS_URL=redis://:browseros_redis_password@localhost:6379

# JWT Configuration
JWT_SECRET=your_jwt_secret_change_in_production
JWT_REFRESH_SECRET=your_jwt_refresh_secret_change_in_production
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=30d

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# File Storage (S3/MinIO)
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=browseros_minio
S3_SECRET_KEY=browseros_minio_password
S3_BUCKET=browseros-files
S3_REGION=us-east-1
